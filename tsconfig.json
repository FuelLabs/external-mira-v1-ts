  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "bundler",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
